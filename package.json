{
  "name": "liff_front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "set-env": "node scripts/setEnv.js",
    "set-env:test": "node scripts/setEnv.js test",
    "set-env:prod": "node scripts/setEnv.js prod",
    "set-env:bvs": "node scripts/setEnv.js bvs",  
    "build": "npm run set-env && next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "firebase use test && npm run set-env && next build && firebase deploy --project test-8120f",
    "deploy:test": "firebase use test && npm run set-env:test && next build && firebase deploy --project test-8120f",
    "deploy:prod": "firebase use prod && npm run set-env:prod && next build && firebase deploy --project shootsundayfront",
    "deploy:bvs": "firebase use bvsfront && npm run set-env:bvs && next build && firebase deploy --project bvsfront"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@line/liff": "^2.23.2",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.0",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "@mui/x-data-grid": "^7.27.2",
    "@mui/x-date-pickers": "^7.27.1",
    "@next/third-parties": "^14.2.4",
    "date-fns": "^2.29.3",
    "dotenv": "^16.4.7",
    "googleapis": "^146.0.0",
    "next": "^14.2.24",
    "open": "^10.1.0",
    "react": "^18",
    "react-calendar": "^5.1.0",
    "react-dom": "^18",
    "react-swipeable": "^7.0.2",
    "styled-components": "^6.1.11",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.2"
  },
  "config": {
    "firebase_project_test": "test-8120f",
    "firebase_project_prod": "shootsundayfront",
    "firebase_project_bvs": "bvsfront"
  }
}
